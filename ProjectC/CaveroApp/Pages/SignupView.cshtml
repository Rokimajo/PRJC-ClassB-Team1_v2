@page
@model CaveroApp.Pages.SignupView

@{
}

<!DOCTYPE html>
<html>
  <head>
    <link rel="stylesheet" href="css/globals.css" />
    <link rel="stylesheet" href="css/styleguide.css"/>
    <link rel="stylesheet" href="css/style.css" />
  </head>
  <body>
    <div class="signup-page">
      <div class="overlap-wrapper">
        <div class="overlap">
          <div class="sign-up-block">
            <div class="overlap-group">
              <img class="cavero-logo-full" src="img/cavero-logo-full.png" />
              <p class="text-wrapper">Velden met * zijn verplicht</p>
            </div>
            <div class="div">Creëer uw account</div>
            <p class="p">Heeft u al een account?</p>
            <div class="log-in-hyperlink">Log in</div>
            <div class="text-wrapper-2">Voornaam*</div>
            <div class="text-wrapper-3">Achternaam*</div>
            <div class="text-wrapper-4">Email*</div>
            <div class="text-wrapper-5">Wachtwoord*</div>
            <div class="text-wrapper-6">Wachtwoord herhalen*</div>
            <div class="create-account">
              <div class="text-wrapper-7">Creëer account</div>
              <img class="arrow" src="img/arrow.png" />
            </div>
            <div class="ellipse"></div>
            <div class="password-repeat-form"></div>
            <div class="password-form"></div>
            <div class="last-name-form"></div>
            <div class="first-name-form"></div>
            <div class="email-form"></div>
          </div>
          <div class="block-breakup">
            <div class="overlap-2">
              <div class="ellipse-2"></div>
              <div class="ellipse-3"></div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </body>
</html>